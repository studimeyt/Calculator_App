<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <Button
        android:id="@+id/one10"
        android:layout_width="101dp"
        android:layout_height="101dp"
        android:background="@drawable/round_button"
        android:text="@string/multiplication"
        android:textAlignment="center"
        android:textColor="#ffffff"
        android:textSize="33sp"
        app:iconPadding="0dp"
        app:layout_constraintBottom_toTopOf="@+id/one12"
        app:layout_constraintEnd_toEndOf="parent" />

    <Button
        android:id="@+id/one12"
        android:layout_width="101dp"
        android:layout_height="101dp"
        android:background="@drawable/round_button"
        android:text="@string/substraction"
        android:textAlignment="center"
        android:textColor="#ffffff"
        android:textSize="33sp"
        app:iconPadding="0dp"
        app:layout_constraintBottom_toTopOf="@+id/one14"
        app:layout_constraintEnd_toEndOf="parent" />

    <Button
        android:id="@+id/one13"
        android:layout_width="101dp"
        android:layout_height="101dp"
        android:background="@drawable/round_button"
        android:text="@string/vault"
        android:textAlignment="center"
        android:textColor="#ffffff"
        android:textSize="33sp"
        app:iconPadding="0dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />

    <Button
        android:id="@+id/one14"
        android:layout_width="101dp"
        android:layout_height="101dp"
        android:background="@drawable/round_button"
        android:text="@string/addition"
        android:textAlignment="center"
        android:textColor="#ffffff"
        android:textSize="33sp"
        app:iconPadding="0dp"
        app:layout_constraintBottom_toTopOf="@+id/one13"
        app:layout_constraintEnd_toEndOf="parent" />

    <Button
        android:id="@+id/one9"
        android:layout_width="101dp"
        android:layout_height="101dp"
        android:layout_marginBottom="8dp"
        android:background="@drawable/round_button"
        android:text="@string/division"
        android:textAlignment="center"
        android:textColor="#ffffff"
        android:textSize="33sp"
        app:iconPadding="0dp"
        app:layout_constraintBottom_toTopOf="@+id/one10"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toEndOf="@+id/one8" />

    <Button
        android:id="@+id/one6"
        android:layout_width="101dp"
        android:layout_height="101dp"
        android:layout_marginBottom="8dp"
        android:background="@drawable/round_button"
        android:text="@string/clear"
        android:textAlignment="center"
        android:textColor="#ffffff"
        android:textSize="33sp"
        app:iconPadding="0dp"
        app:layout_constraintBottom_toTopOf="@+id/one5"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/one7"
        android:layout_width="101dp"
        android:layout_height="101dp"
        android:layout_marginBottom="8dp"
        android:background="@drawable/round_button"
        android:text="@string/openBracket"
        android:textAlignment="center"
        android:textColor="#ffffff"
        android:textSize="33sp"
        app:iconPadding="0dp"
        app:layout_constraintBottom_toTopOf="@+id/two5"
        app:layout_constraintStart_toEndOf="@+id/one6" />

    <Button
        android:id="@+id/one8"
        android:layout_width="101dp"
        android:layout_height="101dp"
        android:layout_marginBottom="8dp"
        android:background="@drawable/round_button"
        android:text="@string/closeBracket"
        android:textAlignment="center"
        android:textColor="#ffffff"
        android:textSize="33sp"
        app:iconPadding="0dp"
        app:layout_constraintBottom_toTopOf="@+id/three5"
        app:layout_constraintStart_toEndOf="@+id/one7" />

    <Button
        android:id="@+id/two5"
        android:layout_width="101dp"
        android:layout_height="101dp"
        android:background="@drawable/round_button"
        android:text="@string/_8"
        android:textAlignment="center"
        android:textColor="#ffffff"
        android:textSize="33sp"
        app:barrierMargin="1dp"
        app:layout_constraintBottom_toTopOf="@+id/two4"
        app:layout_constraintStart_toEndOf="@+id/one5"
        tools:ignore="DuplicateSpeakableTextCheck" />

    <Button
        android:id="@+id/three5"
        android:layout_width="101dp"
        android:layout_height="101dp"
        android:background="@drawable/round_button"
        android:text="@string/_9"
        android:textAlignment="center"
        android:textColor="#ffffff"
        android:textSize="33sp"
        app:layout_constraintBottom_toTopOf="@+id/three4"
        app:layout_constraintStart_toEndOf="@+id/two5" />

    <Button
        android:id="@+id/one5"
        android:layout_width="101dp"
        android:layout_height="101dp"
        android:background="@drawable/round_button"
        android:text="@string/_7"
        android:textAlignment="center"
        android:textColor="#ffffff"
        android:textSize="33sp"
        app:iconPadding="0dp"
        app:layout_constraintBottom_toTopOf="@+id/one4"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/two4"
        android:layout_width="101dp"
        android:layout_height="101dp"
        android:background="@drawable/round_button"
        android:text="@string/_5"
        android:textAlignment="center"
        android:textColor="#ffffff"
        android:textSize="33sp"
        app:barrierMargin="1dp"
        app:layout_constraintBottom_toTopOf="@+id/two"
        app:layout_constraintStart_toEndOf="@+id/one4"
        tools:ignore="DuplicateSpeakableTextCheck" />

    <Button
        android:id="@+id/one4"
        android:layout_width="101dp"
        android:layout_height="101dp"
        android:background="@drawable/round_button"
        android:text="@string/_4"
        android:textAlignment="center"
        android:textColor="#ffffff"
        android:textSize="33sp"
        app:layout_constraintBottom_toTopOf="@+id/one"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/three4"
        android:layout_width="101dp"
        android:layout_height="101dp"
        android:background="@drawable/round_button"
        android:text="@string/_6"
        android:textAlignment="center"
        android:textColor="#ffffff"
        android:textSize="33sp"
        app:layout_constraintBottom_toTopOf="@+id/three"
        app:layout_constraintStart_toEndOf="@+id/two4" />

    <Button
        android:id="@+id/two"
        android:layout_width="101dp"
        android:layout_height="101dp"
        android:background="@drawable/round_button"
        android:text="@string/_2"
        android:textAlignment="center"
        android:textColor="#ffffff"
        android:textSize="33sp"
        app:layout_constraintBottom_toTopOf="@+id/zero"
        app:layout_constraintStart_toEndOf="@+id/one"
        tools:ignore="DuplicateSpeakableTextCheck" />

    <Button
        android:id="@+id/one"
        android:layout_width="101dp"
        android:layout_height="101dp"
        android:background="@drawable/round_button"
        android:text="@string/_1"
        android:textAlignment="center"
        android:textColor="#ffffff"
        android:textSize="33sp"
        app:layout_constraintBottom_toTopOf="@+id/decimal"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/three"
        android:layout_width="101dp"
        android:layout_height="101dp"
        android:background="@drawable/round_button"
        android:text="@string/_3"
        android:textAlignment="center"
        android:textColor="#ffffff"
        android:textSize="33sp"
        app:layout_constraintBottom_toTopOf="@+id/equal"
        app:layout_constraintStart_toEndOf="@+id/two" />

    <Button
        android:id="@+id/decimal"
        android:layout_width="101dp"
        android:layout_height="101dp"
        android:background="@drawable/round_button"
        android:text="@string/decimal"
        android:textAlignment="center"
        android:textColor="#ffffff"
        android:textSize="33sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/zero"
        android:layout_width="101dp"
        android:layout_height="101dp"
        android:background="@drawable/round_button"
        android:text="@string/_0"
        android:textAlignment="center"
        android:textColor="#ffffff"
        android:textSize="33sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toEndOf="@+id/decimal" />

    <Button
        android:id="@+id/equal"
        android:layout_width="101dp"
        android:layout_height="101dp"
        android:background="@drawable/round_button"
        android:text="@string/_equalTo"
        android:textAlignment="center"
        android:textColor="#ffffff"
        android:textSize="33sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toEndOf="@+id/zero" />

</androidx.constraintlayout.widget.ConstraintLayout>